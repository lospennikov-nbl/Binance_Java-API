<binance-messages package="com.binance.api.message.client" signed="true">

  <message name="NewOrder" address="api/v3/order">
    <parameter name="symbol" type="String"/>
    <parameter name="side" type="Side"/>
    <parameter name="type" type="OrderType"/>
    <parameter name="timeInForce" type="TimeInForce"/>
    <parameter name="quantity" type="Double"/>
    <parameter name="price" type="Double"/>
    <optional>
      <parameter name="newClientOrderId" type="String"/>
      <parameter name="stopPrice" type="Double"/>
      <parameter name="icebergQty" type="Double"/>
    </optional>
  </message>

  <message name="OrderTest" address="api/v3/order/test">
    <parameter name="symbol" type="String"/>
    <parameter name="side" type="Side"/>
    <parameter name="type" type="OrderType"/>
    <parameter name="timeInForce" type="TimeInForce"/>
    <parameter name="quantity" type="Double"/>
    <parameter name="price" type="Double"/>
    <optional>
      <parameter name="newClientOrderId" type="String"/>
      <parameter name="stopPrice" type="Double"/>
      <parameter name="icebergQty" type="Double"/>
      <parameter name="recvWindow" type="Long"/>
    </optional>
  </message>

  <message name="QueryOrder" address="api/v3/order">
    <parameter name="symbol" type="String"/>
    <optional>
      <parameter name="orderId" type="Long"/>
      <parameter name="origClientOrderId" type="String"/>
      <parameter name="recvWindow" type="Long"/>
    </optional>
  </message>

  <message name="CancelOrder" address="api/v3/order">
    <parameter name="symbol" type="String"/>
    <optional>
      <parameter name="orderId" type="Long"/>
      <parameter name="origClientOrderId" type="String"/>
      <parameter name="newClientOrderId" type="String"/>
      <parameter name="recvWindow" type="Long"/>
    </optional>
  </message>

  <message name="OpenOrders" address="api/v3/openOrders">
    <parameter name="symbol" type="String"/>
    <optional>
      <parameter name="recvWindow" type="Long"/>
    </optional>
  </message>

  <message name="AllOrders" address="api/v3/allOrders">
    <parameter name="symbol" type="String"/>
    <optional>
      <parameter name="orderId" type="Long"/>
      <parameter name="limit" type="Integer"/>
      <parameter name="recvWindow" type="Long"/>
    </optional>
  </message>

  <message name="Account" address="api/v3/account">
    <optional>
      <parameter name="recvWindow" type="Long"/>
    </optional>
  </message>

  <message name="Trades" address="api/v3/myTrades">
    <parameter name="symbol" type="String"/>
    <optional>
      <parameter name="limit" type="Integer"/>
      <parameter name="fromId" type="Long"/>
      <parameter name="recvWindow" type="Long"/>
    </optional>
  </message>
</binance-messages>