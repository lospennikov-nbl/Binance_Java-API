<binance-messages package="com.binance.api.message.client"  signed="true">
  <message name="Withdraw" address="v1/withdraw.html">
    <parameter name="asset" type="String"/>
    <parameter name="address" type="String"/>
    <parameter name="amount" type="Double"/>
    <optional>
      <parameter name="name" type="String"/>
      <parameter name="recvWindow" type="Long"/>
    </optional>
    <parameter name="timestamp" type="Long"/>
  </message>

  <message name="DepositHistory" address="v1/getDepositHistory.html">
    <optional>
      <parameter name="asset" type="String"/>
      <parameter name="status" type="Integer"/>
      <parameter name="startTime" type="Long"/>
      <parameter name="endTime" type="Long"/>
      <parameter name="recvWindow" type="Long"/>
    </optional>
    <parameter name="timestamp" type="Long"/>
  </message>

  <message name="WithdrawHistory" address="v1/getWithdrawHistory.html">
    <optional>
      <parameter name="asset" type="String"/>
      <parameter name="status" type="Integer"/>
      <parameter name="startTime" type="Long"/>
      <parameter name="endTime" type="Long"/>
      <parameter name="recvWindow" type="Long"/>
    </optional>
    <parameter name="timestamp" type="Long"/>
  </message>
</binance-messages>