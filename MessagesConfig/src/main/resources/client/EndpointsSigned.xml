<binance-messages package="com.binance.api.messages" signed="true">
  <message name="WithdrawMessage" address="wapi/v1/withdraw.html">
    <parameter name="asset" type="String"/>
    <parameter name="address" type="String"/>
    <parameter name="amount" type="Double"/>
    <optional>
      <parameter name="name" type="String"/>
      <parameter name="recvWindow" type="Long"/>
    </optional>
  </message>

  <message name="DepositHistoryMessage" address="wapi/v1/getDepositHistory.html">
    <optional>
      <parameter name="asset" type="String"/>
      <parameter name="status" type="Integer"/>
      <parameter name="startTime" type="Long"/>
      <parameter name="endTime" type="Long"/>
      <parameter name="recvWindow" type="Long"/>
    </optional>
  </message>

  <message name="WithdrawHistoryMessage" address="wapi/v1/getWithdrawHistory.html">
    <optional>
      <parameter name="asset" type="String"/>
      <parameter name="status" type="Integer"/>
      <parameter name="startTime" type="Long"/>
      <parameter name="endTime" type="Long"/>
      <parameter name="recvWindow" type="Long"/>
    </optional>
  </message>
</binance-messages>